class Solution {
public:
    int rob(vector<int>& nums) {
        int dp[100];
        dp[0] = nums[0];
        if((nums.size()-1)==0)
            return dp[0];
        dp[1] = nums[1];
        
        if(nums.size()>2)
            dp[2] = max(nums[0]+nums[2],nums[1]);
        
        for(int i=3;i<=nums.size()-1;i++)
        {
            dp[i] = nums[i]+max(dp[i-2],dp[i-3]);            //考慮前2 3間哪間高 
        }   
        return max(dp[nums.size()-1],dp[nums.size()-2]);
    }
    
};
