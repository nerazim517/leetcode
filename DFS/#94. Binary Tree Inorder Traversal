class Solution {
public:
    vector<int> a;
    vector<int> inorderTraversal(TreeNode* root) {   //LDR
        TreeNode *right = root->right;
        TreeNode *left = root->left;
        
        if(left!=0)
            inorderTraversal(left);     //l
        cout<<root->val;
        a.push_back(root->val);         //d
        if(right!=0)
            inorderTraversal(right);    //r
         
        return a;
    }
};
